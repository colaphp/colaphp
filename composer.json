{
  "name": "daophp/daophp",
  "type": "project",
  "description": "High performance HTTP Service Framework.",
  "keywords": [
    "high performance",
    "http service"
  ],
  "license": "Apache-2.0",
  "require": {
    "php": "^8.0.2",
    "daophp/framework": "^1.0.14",
    "doctrine/migrations": "^3.4",
    "phpoffice/phpspreadsheet": "^1.21"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
