{
  "name": "daosoft/daophp",
  "type": "project",
  "description": "High performance HTTP Service Framework.",
  "keywords": [
    "high performance",
    "http service"
  ],
  "license": "MIT",
  "homepage": "http://www.daophp.com",
  "support": {
    "issues": "https://github.com/daosoft/daophp/issues",
    "source": "https://github.com/daosoft/daophp"
  },
  "require": {
    "php": "^7.2.5",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "doctrine/annotations": "^1.10",
    "firebase/php-jwt": "^5.2",
    "gregwar/captcha": "^1.1",
    "illuminate/database": "^7.27",
    "illuminate/events": "^7.27",
    "illuminate/redis": "^7.27",
    "intervention/image": "^2.5",
    "jenssegers/blade": "^1.3",
    "monolog/monolog": "^2.1",
    "nikic/fast-route": "^1.3",
    "overtrue/wechat": "^4.2",
    "php-di/php-di": "^6.2",
    "vlucas/phpdotenv": "^4.1.8",
    "workerman/validation": "^3.0",
    "workerman/workerman": "^4.0",
    "yansongda/pay": "^2.9"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Swift\\": "support/"
    },
    "files": [
      "support/Support/helpers.php"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "suggest": {
    "ext-event": "For better performance. "
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
