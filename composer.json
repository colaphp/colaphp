{
    "name": "phpmall/phpmall",
    "type": "project",
    "description": "一款开源的多商户商城系统",
    "keywords": [
        "e-commerce",
        "phpmall",
        "B2B2C",
        "high performance"
    ],
    "homepage": "https://www.phpmall.net/",
    "license": "Apache-2.0",
    "require": {
        "php": "^8.1",
        "phpmall/framework": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "easywechat-composer/easywechat-composer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "libraries/"
        }
    ]
}
