{
  "name": "phpmall/framework",
  "description": "High performance HTTP Service Framework.",
  "keywords": ["framework", "phpmall"],
  "license": "MIT",
  "homepage": "https://www.phpmall.net/framework",
  "support": {
    "issues": "https://github.com/phpmall/framework/issues",
    "source": "https://github.com/phpmall/framework"
  },
  "require": {
    "php": "^8.1",
    "ext-fileinfo": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "doctrine/annotations": "^1.13",
    "doctrine/migrations": "^3.5",
    "firebase/php-jwt": "^6.2",
    "gregwar/captcha": "^1.1.9",
    "illuminate/database": "^9.17",
    "illuminate/events": "^9.17",
    "illuminate/pagination": "^9.17",
    "illuminate/redis": "^9.17",
    "illuminate/view": "^9.17",
    "intervention/image": "^2.7.2",
    "monolog/monolog": "^2.0",
    "nikic/fast-route": "^1.3",
    "overtrue/easy-sms": "^2.2",
    "overtrue/pinyin": "^4.0",
    "php-di/php-di": "^6.4",
    "phpoffice/phpspreadsheet": "^1.23",
    "symfony/cache": "^6.1",
    "symfony/mailer": "^6.1",
    "vlucas/phpdotenv": "^5.4",
    "w7corp/easywechat": "^5.27",
    "workerman/crontab": "^1.0",
    "workerman/gateway-worker": "^3.0",
    "workerman/redis-queue": "^1.0",
    "workerman/validation": "^3.0",
    "yansongda/pay": "^3.1"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.8"
  },
  "autoload": {
    "psr-4": {
      "Swift\\": "src/"
    },
    "files": [
      "src/Support/helpers.php"
    ]
  },
  "suggest": {
    "ext-event": "For better performance. "
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
